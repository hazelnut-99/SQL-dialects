WITH d(y) AS (
	SELECT UNNEST(range(
		'2023-05-11 4:00:00'::TIMESTAMP,
		'2023-05-11 4:00:00'::TIMESTAMP + TO_DAYS(7),
		TO_HOURS(6)
	))
)
SELECT
	y,
	y - ('2023-05-11 4:00:00'::TIMESTAMP) AS x
FROM d
WHERE
	x BETWEEN TO_HOURS(-44) AND TO_HOURS(44);
WITH d(y) AS (
	SELECT UNNEST(range(
		'2023-05-11 4:00:00'::TIMESTAMP,
		'2023-05-11 4:00:00'::TIMESTAMP + TO_DAYS(7),
		TO_HOURS(6)
	))
)
SELECT
	y,
	y - ('2023-05-11 4:00:00'::TIMESTAMP) AS x
FROM d
WHERE
	x >= TO_HOURS(-44) AND x <= TO_HOURS(44);
