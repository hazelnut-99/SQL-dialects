CREATE VIEW lists AS
    SELECT CASE
            WHEN c0 = 'b' THEN LIST_VALUE(c0)
            WHEN c0 IS NULL THEN NULL
            ELSE LIST_VALUE(c0, c1, c2, c3, c4)
            END AS s,
    off, length FROM listdata;
