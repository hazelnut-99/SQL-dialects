PRAGMA enable_verification;
create table t (n text);;
insert into t values ('1'),('0'),('');;
select n, case 
when n <> '' and cast(substr(n, 1, 1) as int) <= 0 then '0' 
when n <> '' and cast(substr(n, 1, 1) as int) > 0 then '1' 
else '2'end as x from t
ORDER BY n;;
