  TeamName,
  Player,
  Score,
  NTILE() OVER (PARTITION BY TeamName ORDER BY Score ASC) AS NTILE
FROM ScoreBoard s
ORDER BY TeamName, Score;;
SELECT
  TeamName,
  Player,
