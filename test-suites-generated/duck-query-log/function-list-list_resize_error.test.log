PRAGMA enable_verification;;
prepare q1 as select list_resize(?, ?);;
prepare q2 as select array_resize(?, ?);;
execute q1([1, 2, 3], -1);;
execute q1([1, 2, 3], 'a');;
execute q1('cici n est pas een list', 2);;
execute q1([1, 2, 3], 'huh');;
execute q1([1, 2, 3]);;
execute q2([1, 2, 3], -1);;
execute q2([1, 2, 3], 'a');;
execute q2('cici n est pas een list', 2);;
execute q2([1, 2, 3], 'huh');;
execute q2([1, 2, 3]);;
prepare q3 as select list_resize(?, ?, ?);;
prepare q4 as select array_resize(?, ?, ?);;
execute q3([1, 2, 3], 5, 'abc');;
execute q3([1, 2, 3], 2, 3, 4);;
execute q3([1, 2, 3], 1, [1, 2, 3]);;
execute q4([1, 2, 3], 5, 'abc');;
execute q4([1, 2, 3], 2, 3, 4);;
execute q4([1, 2, 3], 1, [1, 2, 3]);;
